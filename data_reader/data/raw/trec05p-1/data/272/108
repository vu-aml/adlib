	document.write('<font color=#FF9933><b>::</b></font>');
	document.write('&nbsp;'+comment+'&nbsp;');
	document.write('<font color=#FF9933><b>::</b></font>&nbsp;');
	document.write('</td></tr></table></td></tr></table>');
}

