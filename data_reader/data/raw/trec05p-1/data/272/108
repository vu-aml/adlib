function header_2(H_string,comment)
{
	document.write('<table bgcolor=#FF9933 width=100%>');
	document.write('<tr><td class=header1>&nbsp;');
	document.write(H_string.charAt(0));
	document.write('&nbsp;</td><td width=100%>');
