#!/usr/bin/perl
use strict;
use warnings;
use Data::Dumper;

$/ = "\n\n"; # Specify the record separator as 2 new lines..
my $fn = 'detail-20070423_1.txt';
open my $fh, '<', $fn or die $!;

while(<$fh>){
    my %test;
    map {
        my ($fn,$val) = split(/=/,$_,2);
        $fn =~ s/^\s*//g;
        $fn =~ s/\s*$//g;
        $val =~ s/^\s*"?//g if defined $val;
        $val =~ s/"?\s*//g if defined $val;
        $test{$fn}=$val;
    } split(/\n/, $_);
    print Dumper(\%test);
}

