<script>

	var newClick =3D 'WB1';
	var classname=3D "";
	var parentFrameLength =3D 0;	// =BB=F3=C0=A7 =C7=C1=B7=B9=C0=D3 =B0=B9=BC=
=F6

	function checkcontained()
	{
		getParentFrameLength();

		if(newClick !=3D '')
			checkHrefTag();
		else
			checkSpanTag();
	}

	function getParentFrameLength()
	{
		var iFrameCount =3D 0;
		try{
		    //=BD=C7=C7=E0=C7=D2 =B3=BB=BF=EB=B5=E9
			for(i=3D0; i < parent.frames.length; i++)
			{
				if((parent.frames[i].name !=3D 'header') && (parent.frames[i].name !=3D=
 'content'))
					parentFrameLength++;
			}

		}catch(e){
		   //=BF=A1=B7=AF=B0=A1 =B9=DF=BB=FD=C7=DF=C0=BB=B6=A7 =C3=B3=B8=AE=C7=D2=
 =B3=BB=BF=EB
		   return;
		}
	}

	function checkHrefTag()
	{
		var cur =3D event.srcElement;

		while (cur.parentElement)
		{
			var TagName =3D cur.tagName;

			//alert("checkHrefTag TagName =3D=3D=3D=3D=3D=3D=3D=3D=3D=3D "+TagName)=
;

			if(TagName =3D=3D 'A' || TagName =3D=3D 'AREA')
			{

				var myPage =3D document.location.href;
				var hValue =3D cur.href;

				if(hValue.indexOf('javascript') > -1)
					return;

				if(hValue.indexOf('NEWCLICK') > -1)
					return;

				if(myPage !=3D hValue)
				{
					var shapValue =3D "";

					if(hValue.indexOf('#') > -1)
					{
						shapValue =3D hValue.substring(hValue.indexOf('#'));
						hValue =3D hValue.substring(0,hValue.indexOf('#'));
					}

					if(hValue.indexOf('?') > -1)
						hValue =3D hValue+'&NEWCLICK=3D'+newClick+shapValue;
					else
						hValue =3D hValue+'?NEWCLICK=3D'+newClick+shapValue;

					//alert("* parentFrameLength : "+parentFrameLength);

					if((cur.target.toLowerCase() !=3D '_new') && (cur.target.toLowerCase(=
) !=3D '_blank'))
					{
						if(parentFrameLength > 0)
							cur.target =3D "_parent";
						else
							cur.target =3D "";
					}

					cur.href =3D hValue;
				}

				return;

			} else {
				cur=3D cur.parentElement;
			}
		}
	}

	function checkSpanTag()
	{
		cur =3D event.srcElement;

		while (cur.parentElement)
		{
			if (cur.id=3D=3D"CHECKCODE")
			{
				//alert(cur.className);
				classname =3D cur.className;
				break;
			}

			cur=3D cur.parentElement;
		}

		if(classname =3D=3D '')
			checkHrefTag();
	}

	function gotoBook(bKind, barcode)
	{
		var cntForm =3D 0;

		for(i=3D0; i<document.forms.length;i++)
		{
			if(document.forms[i].name =3D=3D'checkForm')
				cntForm +=3D 1;
		}

		if(cntForm > 1)
			forms =3D document.checkForm[0];
		else
			forms =3D document.checkForm;

		forms.BKIND.value 	=3D bKind;
		forms.BARCODE.value =3D barcode;

		if(newClick =3D=3D '')
			forms.NEWCLICK.value   =3D classname;

		if(parentFrameLength > 0)
			forms.target =3D "_parent";
		else
			forms.target =3D "";

		if(bKind =3D=3D 'DWN')
			page =3D "/digital/dwshop/shopdetail/DwDetailView.jsp";
		else
			page =3D "/category/bookdetail/BookDetailView.jsp";

		forms.action =3D page;
		forms.submit();
	}

	function gotoNextPage(url)
	{
		var clickCode =3D newClick;

		if(clickCode =3D=3D '')
			clickCode =3D classname;

		if(url.indexOf("?") > -1)
			url =3D url+"&NEWCLICK=3D"+clickCode;
		else
			url =3D url+"?NEWCLICK=3D"+clickCode;

		if(parentFrameLength > 0)
			parent.document.location.href =3D url;
		else
			window.document.location.href =3D url;
	}

	function commonListMenu(targ,selObj,restore)
	{
	  	myPage =3D eval(targ+".location.href");

		if(myPage.length > 0)
		{
			pos =3D myPage.indexOf("NEWCLICK");

			if( (pos > -1) && (myPage.length <=3D pos+12 ) )
				newClick =3D myPage.substring(pos+9, pos+12);
			else
			{
				checkSpanTag();
				newClick =3D classname;
			}
		}

	  	if(newClick !=3D "")
		  	eval(targ+".location=3D'"+selObj.options[selObj.selectedIndex].value+=
"?NEWCLICK=3D"+newClick+"'");
		else
		  	eval(targ+".location=3D'"+selObj.options[selObj.selectedIndex].value+=
"'");

	  	if (restore) selObj.selectedIndex=3D0;
	}

	document.onclick=3Dcheckcontained;

